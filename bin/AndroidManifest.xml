<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rehapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rh_launcher"
        android:label="@string/app_name"
        android:launchMode="singleTop"
        android:theme="@style/RHTheme" >
        <activity
            android:name="com.example.rehapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.rehapp.HomeActivity"
            android:label="@string/title_home_activity" >
        </activity>
        
        <activity
            android:name="com.example.rehapp.ExercisesActivity"
            android:label="@string/title_activity_exercises"
            android:parentActivityName="com.example.RehApp.HomeActivity" >
        </activity>
        
        <activity
            android:name="com.example.rehapp.Schedule"
            android:label="@string/title_activity_schedule"
            android:parentActivityName="com.example.RehApp.HomeActivity" >
        </activity>        
        
		<activity
            android:name="com.example.rehapp.DoctorNotes"
            android:label="@string/title_activity_doctor_notes" 
            android:parentActivityName="com.example.HomeActivity" >
        </activity>       
        
        <activity
            android:name="com.example.rehapp.Photos"
            android:label="@string/title_activity_photos"
            android:parentActivityName="com.example.HomeActivity" >
        </activity>
        
    </application>

</manifest>
